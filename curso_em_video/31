from deletime import date
ano = int(input("que ano quer analzar? coloque 0 para analizar o ano atual: "))
if ano == 0:
    ano = date.today(),year
if ano % 4 == 0 and ano % 100 != 0 or ano % 400 == 0:
    print("o ano {} é BISSEXTO".format(ano))
else:
    print("o ano {} NAO É BISSEXTO".format(ano))